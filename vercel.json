{
  "version": 2,
  "cleanUrls": true,
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/",
      "dest": "/app/templates/index.html"
    },
    {
      "src": "/menu",
      "dest": "/app/templates/menu.html"
    },
    {
      "src": "/ligas",
      "dest": "/app/templates/ligas.html"
    },
    {
      "src": "/equipo",
      "dest": "/app/templates/equipo.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app/templates/index.html"
    }
  ]
}