{
  "version": 2,
  "name": "scoutgine-frontend",
  "builds": [
    {
      "src": "app/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/app/templates/$1"
    },
    {
      "src": "/",
      "dest": "/app/templates/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ]
}